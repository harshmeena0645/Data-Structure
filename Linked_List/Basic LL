class Node:
    def __init__(self,data):
        self.data=data
        self.ref=None

class LL:
    def __init__(self):
        self.head=None
    def Print_LL(self):
        if self.head is None:
            print("Linked List is Empty")
        else:
            n=self.head
            while n is not None:
                n=n.ref

LL1=LL()
LL1.Print_LL()


Output:
Linked List is Empty
