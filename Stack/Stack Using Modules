# Collection --> Deque

import collections
stack = collections.deque()

def push():
    a=int(input("Element:"))
    stack.append(a)
    print(stack)
        
def pop_element():
    d=stack.pop()
    print("Delete item is :",d)
    print(stack)

while True:
    print("Select The Operation:")
    print("1. Push")
    print("2. Pop")
    print("3. Quit")
    choice=int(input("Your Choice:"))
    if choice==1:
        push()
    elif choice==2:
        pop_element()
    elif choice==3:
        break
    else:
        print("Enter Correct Operation")




# Queue --> LifoQueue

import queue
stack = queue.LifoQueue(3)

def put_element():
    a=int(input("Element:"))
    stack.put(a,timeout=1)
    print(stack)
        
def get_element():
    d=stack.get(timeout=1)
    print("Delete item is :",d)
    print(stack)

while True:
    print("Select The Operation:")
    print("1. Push")
    print("2. Pop")
    print("3. Quit")
    choice=int(input("Your Choice:"))
    if choice==1:
        put_element()
    elif choice==2:
        get_element()
    elif choice==3:
        break
    else:
        print("Enter Correct Operation")
